#=
*******************************************
 * File: Bitwise
 * A test program for relational operators
 *******************************************
=#

x::Int = 10
y::Int = 20

print("x = ",x,"\n")
print("y = ",y,"\n")

print ("x < y && x > y = ", x<y && x>0 ,"\n");

print("~x = ",~x,"\n");

print("~y = ",~y,"\n");

print ("x & y = ",x & y,"\n")

print ("x | y = ",x | y,"\n")

print ("x $ y = ",x $ y,"\n")

print ("x >> y = ",x >> y,"\n")

print ("x << y = ",x << y,"\n")



