a::Array{Int} = [5,4,2,1,3]
temp::Int = a[2]
a[2] = 7
println (temp)

#=
for i = 1:5
    for j = 2:5-i
        if(a[j-1] > a[j])
            temp = a[j-1]
            a[j-1] = a[j]
            a[j]=temp
        end
    end
   println(a[j])
end
=#


b::Array{Bool} = [true,false,false,true,true,false]

println("Imprimiendo Booleans")

for j = 1:6
   println(b[j])
end