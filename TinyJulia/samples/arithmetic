x::Int = 10
y::Int = 3
z::Bool = false
println ("x = ",x)
println ("y = ",y)
println ("-x = ",-x)
println ("-y = ",-y);println ("x+y = ",x + y); println("x+true = ",x+true)
println ("x-y = ",x - y)
println ("x*y = ",x * y)
println ("x/y = ",x / y)
println ("x%y = ",x % y)
println ("x^y = ",x ^ y)